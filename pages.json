{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"enablePullDownRefresh": true, //开启触底
				"onReachBottomDistance": 10 //距离底部多少默认50px
			}
		}, {
			"path": "pages/cart/cart",
			"style": {
				"navigationBarTitleText": "购物车"
			}

		}, {
			"path": "pages/member/member",
			"style": {
				"navigationBarTitleText": "我的"
			}

		}, {
			"path": "pages/news/news",
			"style": {
				"navigationBarTitleText": "咨询"
			}

		}, {
			"path": "pages/goods/goods",
			"style": {
				"navigationBarTitleText": "商品列表",
				"enablePullDownRefresh": true
			}

		}, {
			"path": "pages/contact/contact",
			"style": {
				"navigationBarTitleText": "联系我们"
			}

		}, {
			"path": "pages/pics/pics",
			"style": {
				"navigationBarTitleText": "社区图片"
			}

		}, {
			"path": "pages/videos/videos",
			"style": {
				"navigationBarTitleText": "学习视频"
			}

		}, {
			"path": "pages/news_detail/news_detail",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/goods_detail/goods_detail",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/setting/setting",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "京东",
		"navigationBarBackgroundColor": "#ff0036",
		"backgroundColor": "#F8F8F8"

	},
	"tabBar": {
		"selectedColor": "#d81e06", //选中时文字颜色，默认蓝色
		"color": "#000000", //未选择时文字颜色
		"list": [{
				"text": "首页",
				"pagePath": "pages/index/index",
				"iconPath": "static/tabber/home.png",
				"selectedIconPath": "static/tabber/home(1).png"
			},
			{
				"text": "咨询",
				"pagePath": "pages/news/news",
				"iconPath": "static/tabber/earth.png",
				"selectedIconPath": "static/tabber/earth(1).png"
			},
			{
				"text": "购物车",
				"pagePath": "pages/cart/cart",
				"iconPath": "static/tabber/carts.png",
				"selectedIconPath": "static/tabber/carts(1).png"
			},
			{
				"text": "我的",
				"pagePath": "pages/member/member",
				"iconPath": "static/tabber/my.png",
				"selectedIconPath": "static/tabber/my(1).png"
			}
		]
	}
}
